<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" minWidth="200.0" prefHeight="96.0" prefWidth="400.0" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="image" fitHeight="96.0" fitWidth="96.0" pickOnBounds="true" preserveRatio="true" />
      <Label id="label" fx:id="label" maxWidth="999" prefWidth="1.0" style="-fx-background-color: #ff0;" text="9" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets left="8.0" />
         </HBox.margin>
         <font>
            <Font size="32.0" />
         </font></Label>
      <Region prefHeight="90.0" prefWidth="999.0" />
      <Button id="up" mnemonicParsing="false" onAction="#reorderUp" prefHeight="90.0" text="▲">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button id="down" mnemonicParsing="false" onAction="#reorderDown" prefHeight="90.0" text="▼">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button id="delete" mnemonicParsing="false" onAction="#deleteFrame" prefHeight="90.0" text="⨯">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <font>
            <Font size="16.0" />
         </font></Button>
      <HBox prefHeight="100.0" prefWidth="200.0" />
   </children>
   <opaqueInsets>
      <Insets bottom="8.0" />
   </opaqueInsets>
   <cursor>
      <Cursor fx:constant="OPEN_HAND" />
   </cursor>
   <padding>
      <Insets left="8.0" right="16.0" />
   </padding>
</fx:root>
